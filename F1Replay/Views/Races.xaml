<Page x:Class="WpfApp1.Views.Races"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Races" Height="auto" Width="auto">
    <!-- Import Window Resources-->
    <Page.Resources>
        <!-- Parent "Single Source" Dictionary-->
        <ResourceDictionary>
            <!-- Item To Merge Dictionaries-->
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Styles.xaml"/>
                <ResourceDictionary Source="/Resources/Strings.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <Grid>
        <TextBlock Style="{StaticResource DarkTextBlockStyle}" Text="{StaticResource Race_Content}"/>
    </Grid>
</Page>
